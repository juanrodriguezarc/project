pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'echo "Hello Build"'
            }
        }
        stage('Test') {
            steps {
                echo 'echo "Hello Test"'
            }
        }
        stage('Deploy') {
            steps {
                echo 'echo "Hello Deploy"'
            }
        }
    }
}